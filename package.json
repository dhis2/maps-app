    "script-loader": "^0.7.2",
    "style-loader": "^0.18.2",
    "susy": "^3.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "pre-commit": [
    "validate",
    "test"
  ],
  "manifest.webapp": {
    "name": "Maps App",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "Bjørn Sandvik"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
