        "optipng-bin": "^4.0.0",
        "pngquant-bin": "^3.1.1",
        "precommit-hook": "3.0.0",
        "prettier": "1.11.1",
        "react-addons-test-utils": "^15.6.0",
        "recursive-readdir": "^2.2.2",
        "redux-devtools": "^3.4.0",
        "sass-loader": "^6.0.6",
        "script-loader": "^0.7.2",
        "style-loader": "^0.20.2",
        "susy": "^3.0.3",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.1",
        "webpack-uglify-js-plugin": "^1.1.9"
    },
    "pre-commit": [
        "validate",
        "test"
    ],
    "manifest.webapp": {
        "name": "DHIS2 Maps",
        "icons": {
            "48": "icon.png"
        },
        "developer": {
