{
    "name": "maps-app",
    "version": "31.0.6",
    "description": "DHIS2 Maps",
    "main": "src/app.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhis2/maps-app.git"
    },
    "license": "BSD-3-Clause",
    "author": "Bjørn Sandvik",
    "husky": {
        "hooks": {
            "pre-commit": "npm run extract-pot && npm run prettify && CI=true npm run test && git add -A ."
        }
    },
    "pre-commit": [
        "validate",
        "test"
    ],
    "scripts": {
        "prestart": "yarn localize && d2-manifest package.json manifest.webapp",
        "start": "webpack-dev-server",
        "coverage": "yarn test --coverage",
        "test": "jest",
        "test:watch": "yarn test --watch",
        "prebuild": "yarn test",
        "build": "yarn localize && rm -rf build && NODE_ENV=production webpack --progress && npm run manifest && cp -r i18n build && cp -r i18n_old build",
        "postbuild": "cp icon.png ./build/ && cp -r images ./build && cp ./package.json ./build/package.json",
        "manifest": "d2-manifest package.json build/manifest.webapp",
        "lint": "eslint ./src",
        "validate": "npm ls --depth 0 || yarn list",
        "profile": "yarn start --profile",
        "prettify": "prettier \"src/**/*.{js,jsx,json,css}\" --write",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/"
    },
    "dependencies": {
        "@dhis2/d2-i18n": "^1.0.4",
        "@dhis2/d2-ui-file-menu": "^2.0.0",
        "@dhis2/d2-ui-interpretations": "^1.1.12",
        "@dhis2/d2-ui-org-unit-tree": "^1.0.13",
        "@dhis2/gis-api": "30.0.0",
        "@material-ui/core": "^3.0.3",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/lab": "^3.0.0-alpha.15",
        "babel-preset-stage-0": "^6.24.1",
        "d2": "29.1.5",
        "d2-utilizr": "^0.2.15",
        "d3-color": "^1.2.3",
        "d3-format": "^1.3.2",
        "d3-time-format": "^2.1.3",
        "enzyme-to-json": "^3.3.4",
        "file-saver": "^1.3.3",
        "husky": "next",
        "lodash": "^4.17.5",
        "loglevel": "^1.6.1",
        "material-ui": "^0.20.0",
        "material-ui-icons": "^1.0.0-beta.17",
        "promise-polyfill": "^8.1.0",
        "prop-types": "^15.6.2",
        "react": "16.5.0",
        "react-color": "^2.13.5",
        "react-dom": "16.5.0",
        "react-jss": "^8.6.1",
        "react-redux": "^5.0.7",
        "react-sortable-hoc": "^0.8.3",
        "react-virtualized": "^9.20.1",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-observable": "^0.18.0",
        "redux-thunk": "^2.3.0",
        "rxjs": "5.5.6",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-jest": "^22.4.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.4",
        "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.10",
        "@dhis2/d2-i18n-extract": "^1.0.7",
        "@dhis2/d2-i18n-generate": "^1.0.19",
        "d2-manifest": "^1.0.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.18.1",
        "eslint-config-dhis2": "^3.0.7",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.7.0",
        "exports-loader": "^0.7.0",
        "file-loader": "^1.1.9",
        "gifsicle": "^3.0.4",
        "html-webpack-plugin": "^2.30.1",
        "image-webpack-loader": "^4.1.0",
        "imports-loader": "^0.8.0",
        "isparta-loader": "^2.0.0",
        "jest": "^22.4.2",
        "node-sass": "^4.5.3",
        "nyc": "^11.4.1",
        "optipng-bin": "^4.0.0",
        "pngquant-bin": "^3.1.1",
        "precommit-hook": "3.0.0",
        "prettier": "^1.13.4",
        "react-addons-test-utils": "^15.6.0",
        "recursive-readdir": "^2.2.2",
        "redux-devtools": "^3.4.0",
        "sass-loader": "^6.0.6",
        "script-loader": "^0.7.2",
        "style-loader": "^0.20.2",
        "susy": "^3.0.3",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-server": "^2.11.1",
        "webpack-uglify-js-plugin": "^1.1.9"
    },
    "manifest.webapp": {
        "name": "DHIS2 Maps",
        "icons": {
            "48": "icon.png"
        },
        "developer": {
            "url": "",
            "name": "Bjørn Sandvik"
        },
        "activities": {
            "dhis": {
                "href": ".."
            }
        }
    }
}
