{
    "name": "maps-app",
    "version": "32.0.1",
    "description": "DHIS2 Maps",
    "main": "src/app.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhis2/maps-app.git"
    },
    "license": "BSD-3-Clause",
    "author": "Bjørn Sandvik",
    "husky": {
        "hooks": {
            "pre-commit": "yarn extract-pot && yarn prettify && CI=true yarn test && git add -A ."
        }
    },
    "pre-commit": [
        "validate",
        "test",
        "lint"
    ],
    "scripts": {
        "prestart": "yarn localize && d2-manifest package.json manifest.webapp",
        "start": "webpack-dev-server",
        "coverage": "yarn test --coverage",
        "test": "jest",
        "test:watch": "yarn test --watch",
        "cy:verify": "cypress verify",
        "cy:stub:run": "cypress run",
        "cy:stub:open": "cypress open",
        "cy:e2e:open": "CYPRESS_E2E=true cypress open",
        "cy:e2e:run": "CYPRESS_E2E=true cypress run",
        "cy:e2e:update": "CYPRESS_GEN_FIXTURES=true cypress run --config video=false",
        "cy:e2e:open:update": "CYPRESS_GEN_FIXTURES=true cypress open",
        "prebuild": "yarn test",
        "build": "yarn localize && rm -rf build && NODE_ENV=production webpack --progress && yarn manifest && cp -r i18n build && cp -r i18n_old build",
        "postbuild": "cp icon.png ./build/ && cp -r images ./build",
        "manifest": "d2-manifest package.json build/manifest.webapp",
        "lint": "eslint ./src",
        "validate": "npm ls --depth 0 || yarn list",
        "profile": "yarn start --profile",
        "prettify": "prettier \"src/**/*.{js,jsx,json,css}\" --write",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/",
        "analyze-bundle": "webpack --profile --json --progress > .webpack-profile.json && webpack-bundle-analyzer .webpack-profile.json"
    },
    "dependencies": {
        "@dhis2/d2-i18n": "^1.0.4",
        "@dhis2/d2-ui-core": "5.2.6",
        "@dhis2/d2-ui-file-menu": "5.2.6",
        "@dhis2/d2-ui-interpretations": "5.2.6",
        "@dhis2/d2-ui-org-unit-dialog": "5.2.6",
        "@dhis2/d2-ui-org-unit-tree": "5.2.6",
        "@dhis2/gis-api": "^31.0.6",
        "@dhis2/ui": "^1.0.0-beta.12",
        "@material-ui/core": "^3.4.0",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/lab": "^3.0.0-alpha.23",
        "babel-preset-stage-0": "^6.24.1",
        "d2": "31.4.0",
        "d2-utilizr": "^0.2.15",
        "d3-color": "^1.2.3",
        "d3-format": "^1.3.2",
        "file-saver": "^2.0.1",
        "lodash": "^4.17.5",
        "loglevel": "^1.6.1",
        "promise-polyfill": "^8.1.0",
        "prop-types": "^15.6.2",
        "react": "16.8.3",
        "react-color": "^2.13.5",
        "react-dom": "16.8.3",
        "react-jss": "^8.6.1",
        "react-redux": "^6.0.1",
        "react-sortable-hoc": "^1.6.1",
        "react-virtualized": "^9.20.1",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-observable": "^1.0.0",
        "redux-thunk": "^2.3.0",
        "rxjs": "6.4.0",
        "url-polyfill": "^1.1.0",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@dhis2/d2-i18n-extract": "^1.0.7",
        "@dhis2/d2-i18n-generate": "^1.0.19",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-istanbul": "^5.1.1",
        "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-object-assign": "^6.22.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "chai": "^4.1.2",
        "css-loader": "^2.1.0",
        "cypress": "3.1.5",
        "cypress-pipe": "^1.3.2",
        "d2-manifest": "^1.0.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.4",
        "eslint": "^5.14.1",
        "eslint-config-dhis2": "^3.0.7",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "7.12.4",
        "exports-loader": "^0.7.0",
        "file-loader": "^3.0.1",
        "gifsicle": "^4.0.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.1.3",
        "image-webpack-loader": "^4.1.0",
        "imports-loader": "^0.8.0",
        "isparta-loader": "^2.0.0",
        "jest": "^24.1.0",
        "nyc": "^13.3.0",
        "optipng-bin": "^5.1.0",
        "pngquant-bin": "^5.0.2",
        "precommit-hook": "3.0.0",
        "prettier": "^1.13.4",
        "react-addons-test-utils": "^15.6.0",
        "recursive-readdir": "^2.2.2",
        "redux-devtools": "^3.4.0",
        "script-loader": "^0.7.2",
        "style-loader": "^0.23.1",
        "susy": "^3.0.3",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "^3.0.2",
        "webpack-dev-server": "^3.2.1",
        "webpack-uglify-js-plugin": "^1.1.9"
    },
    "manifest.webapp": {
        "name": "DHIS2 Maps",
        "icons": {
            "48": "icon.png"
        },
        "developer": {
            "url": "",
            "name": "Bjørn Sandvik"
        },
        "activities": {
            "dhis": {
                "href": ".."
            }
        }
    }
}
