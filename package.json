{
  "name": "maps-app",
  "version": "29.0.0",
  "description": "DHIS2 Maps app",
  "main": "src/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/maps-app.git"
  },
  "license": "BSD-3-Clause",
  "author": "Bjørn Sandvik",
  "scripts": {
    "prestart": "d2-manifest package.json manifest.webapp",
    "start": "webpack-dev-server",
    "coverage": "npm test -- --coverage",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "prebuild": "yarn test",
    "build": "rm -rf build && NODE_ENV=production webpack --progress && npm run manifest",
    "postbuild": "cp icon.png ./build/ && cp -r images ./build",
    "validate": "npm ls --depth 0 || yarn list",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "deploy": "npm run build && mvn clean deploy",
    "lint": "eslint ./src",
    "profile": "yarn start -- --profile"
  },
  "dependencies": {
    "d2": "^28.2.0",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.0.3",
    "d3-time-format": "^2.0.5",
    "dhis2-gis-api": "^29.0.1",
    "file-saver": "^1.3.3",
    "i18next": "^10.0.7",
    "i18next-xhr-backend": "^1.5.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.5.0",
    "material-ui": "^0.19.1",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-next": "npm:material-ui@next",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-color": "^2.13.5",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-sortable-hoc": "^0.6.8",
    "react-virtualized": "^9.13.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "d2-manifest": "^1.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-dhis2": "^2.1.0",
    "eslint-plugin-react": "^7.3.0",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.2",
    "gifsicle": "^3.0.4",
    "html-webpack-plugin": "^2.30.1",
    "i18next-conv": "^5.0.0",
    "image-webpack-loader": "^3.4.2",
    "imports-loader": "^0.7.1",
    "isparta-loader": "^2.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "nyc": "^11.2.1",
    "optipng-bin": "^4.0.0",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "^0.0.2",
    "pngquant-bin": "^3.1.1",
