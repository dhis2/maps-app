{
  "name": "maps-app",
  "version": "31.0.18",
  "description": "DHIS2 Maps",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/maps-app.git"
  },
  "license": "BSD-3-Clause",
  "author": "Bjørn Sandvik",
  "husky": {
    "hooks": {
      "pre-commit": "yarn extract-pot && yarn prettify && CI=true yarn test && git add -A ."
    }
  },
  "pre-commit": [
    "validate",
    "test-ci",
    "lint"
  ],
  "scripts": {
    "prestart": "yarn localize && d2-manifest package.json manifest.webapp",
    "start": "react-scripts start",
    "coverage": "yarn test --coverage",
    "test": "react-scripts test",
    "test-ci": "CI=true react-scripts test",
    "cy:verify": "cypress verify",
    "cy:stub:run": "cypress run",
    "cy:stub:open": "cypress open",
    "cy:e2e:open": "CYPRESS_E2E=true cypress open",
    "cy:e2e:run": "CYPRESS_E2E=true cypress run",
    "cy:e2e:update": "CYPRESS_GEN_FIXTURES=true cypress run --config video=false",
    "cy:e2e:open:update": "CYPRESS_GEN_FIXTURES=true cypress open",
    "prebuild": "yarn test && yarn localize && rm -rf build",
    "build": "react-scripts build",
    "postbuild": "yarn manifest",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "lint": "eslint ./src",
    "validate": "npm ls --depth 0 || yarn list",
    "profile": "yarn start --profile",
    "prettify": "prettier \"src/**/*.{js,jsx,json,css}\" --write",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./public/i18n/ -o ./src/locales/"
  },
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.4",
    "@dhis2/d2-ui-core": "^5.0.3",
    "@dhis2/d2-ui-file-menu": "^5.0.3",
    "@dhis2/d2-ui-interpretations": "^5.0.3",
    "@dhis2/d2-ui-org-unit-dialog": "5.0.10",
    "@dhis2/d2-ui-org-unit-tree": "^5.0.3",
    "@dhis2/gis-api": "31.0.3",
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.23",
    "d2": "^31.2.1",
    "d2-utilizr": "^0.2.15",
    "d3-color": "^1.2.3",
    "d3-format": "^1.3.2",
    "d3-time-format": "^2.1.3",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-color": "^2.17.0",
    "react-dom": "^16.6.3",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.8.3",
    "react-virtualized": "^9.21.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "5.5.6",
    "ui": "github:d2-ci/ui",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "cypress": "^3.1.4",
    "cypress-pipe": "^1.3.3",
    "d2-manifest": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "husky": "^1.1.3",
    "prettier": "^1.13.4",
    "react-jss": "^8.6.1",
    "react-scripts": "2.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "manifest.webapp": {
    "name": "DHIS2 Maps",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "Bjørn Sandvik"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
