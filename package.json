{
  "name": "@dhis2/maps-app",
  "private": true,
  "version": "34.0.1",
  "description": "DHIS2 Maps",
  "main": "build/cjs/app.js",
  "module": "build/es/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/maps-app.git"
  },
  "license": "BSD-3-Clause",
  "author": "Bj√∏rn Sandvik",
  "scripts": {
    "start": "d2-app-scripts start",
    "coverage": "yarn test --coverage",
    "test": "d2-app-scripts test",
    "test:watch": "yarn test --watch",
    "cy:verify": "cypress verify",
    "cy:stub:run": "wait-on http-get://localhost:8082 && cypress run",
    "cy:stub:open": "wait-on http-get://localhost:8082 && cypress open",
    "cy:e2e:open": "wait-on http-get://localhost:8082 && CYPRESS_E2E=true cypress open",
    "cy:e2e:run": "wait-on http-get://localhost:8082 && CYPRESS_E2E=true cypress run --headless --browser chrome",
    "cy:e2e:update": "wait-on http-get://localhost:8082 && CYPRESS_GEN_FIXTURES=true cypress run --headless --browser chrome --config video=false",
    "cy:e2e:open:update": "wait-on http-get://localhost:8082 && CYPRESS_GEN_FIXTURES=true cypress open",
    "build": "d2-app-scripts build",
    "build:netlify": "yarn build --standalone"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^2.0.4",
    "@dhis2/app-runtime-d2-shim": "file:../../../libs/app-runtime-d2-shim",
    "@dhis2/d2-i18n": "^1.0.6",
    "@dhis2/d2-ui-analytics": "^0.0.3",
    "@dhis2/d2-ui-core": "5.2.10",
    "@dhis2/d2-ui-file-menu": "5.2.10",
    "@dhis2/d2-ui-interpretations": "6.2.1",
    "@dhis2/d2-ui-org-unit-dialog": "5.2.10",
    "@dhis2/d2-ui-org-unit-tree": "5.2.10",
    "@dhis2/gis-api": "^34.0.7",
    "@dhis2/ui-core": "^4.1.1",
    "@dhis2/ui-widgets": "^2.0.5",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.23",
    "d2": "31.2.2",
    "d2-utilizr": "^0.2.15",
    "d3-axis": "^1.0.12",
    "d3-color": "^1.2.3",
    "d3-format": "^1.3.2",
    "d3-scale": "^3.0.0",
    "d3-selection": "^1.4.0",
    "d3-time": "^1.0.11",
    "d3-time-format": "^2.1.3",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.5",
    "loglevel": "^1.6.1",
    "prop-types": "^15.6.2",
    "react-color": "^2.13.5",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.8.3",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "5.5.6"
  },
  "devDependencies": {
    "@dhis2/cli-app-scripts": "^3.2.2",
    "@dhis2/cli-style": "^5.1.0",
    "cypress": "3.8.0",
    "cypress-pipe": "^1.3.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "react-addons-test-utils": "^15.6.0",
    "redux-devtools": "^3.4.0",
    "wait-on": "^3.3.0"
  }
}
