{
    "name": "maps-app",
    "version": "29.0.28",
    "description": "DHIS2 Maps",
    "main": "src/app.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dhis2/maps-app.git"
    },
    "license": "BSD-3-Clause",
    "author": "Bjørn Sandvik",
    "scripts": {
        "prestart": "d2-manifest package.json manifest.webapp",
        "start": "webpack-dev-server",
        "coverage": "npm test -- --coverage",
        "test": "jest",
        "test:watch": "yarn test -- --watch",
        "prebuild": "yarn test",
        "build": "rm -rf build && NODE_ENV=production webpack --progress && npm run manifest && cp -r i18n build",
        "postbuild": "cp icon.png ./build/ && cp -r images ./build",
        "validate": "npm ls --depth 0 || yarn list",
        "manifest": "d2-manifest package.json build/manifest.webapp",
        "deploy": "yarn build && mvn clean deploy",
        "lint": "eslint ./src",
        "profile": "yarn start -- --profile",
        "prettify": "prettier \"src/{**/*,*}.js\" --write"
    },
    "dependencies": {
        "babel-preset-stage-0": "^6.24.1",
        "d2": "^29.1.1",
        "d2-ui": "^29.0.5",
        "d2-ui-favorites": "^1.0.4",
        "d2-utilizr": "^0.2.15",
        "d3-color": "^1.0.3",
        "d3-format": "^1.2.2",
        "d3-time-format": "^2.1.1",
        "dhis2-gis-api": "29.0.9",
        "file-saver": "^1.3.3",
        "i18next": "^10.4.1",
        "i18next-xhr-backend": "^1.5.1",
        "lodash": "^4.17.5",
        "loglevel": "^1.6.1",
        "material-ui": "^0.20.0",
        "material-ui-icons": "^1.0.0-beta.17",
        "promise-polyfill": "^7.1.0",
        "prop-types": "^15.5.10",
        "react": "^16.1.1",
        "react-color": "^2.13.5",
        "react-dom": "^16.1.1",
        "react-redux": "^5.0.7",
        "react-sortable-hoc": "^0.6.8",
        "react-virtualized": "^9.18.5",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-observable": "^0.18.0",
        "redux-thunk": "^2.2.0",
        "rxjs": "^5.5.6",
        "whatwg-fetch": "^2.0.3"
    },
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.46",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-jest": "^22.4.3",
        "babel-loader": "^7.1.2",
        "babel-plugin-istanbul": "^4.1.4",
        "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
